<div class="kt-portlet btopzero">
<div class="kt-portlet__body kt-padding-t-10 kt-padding-b-10">
<div class="kt-section__content table-responsive">
<table class="table kt-margin-b-0">
<thead>
<tr>
<th class="kt-font-bolder kt-font-success">Full Name</th>
<th class="kt-font-bolder kt-font-success">Phone Number</th>
<th class="kt-font-bolder kt-font-success">Email Address</th>
<th class="kt-font-bolder kt-font-success">Status</th>
<th class="kt-font-bolder kt-font-success"></th>
</tr>
</thead>
<tbody>
<% members.forEach(member => { %>
<tr>
<td class="text-capitalize"><%= member.fullname %></td>
<td>0<%= member.phone %></td>
<td><%= member.email %></td>
<td>-</td>
<td class="text-center"><a href="/admin/profile/<%= member.user_id %>" class="btn btn-secondary btn-sm">View</a></td>
</tr>
<% }) %>
</tbody>
</table>
</div>
</div>
<!--end of kt-portlet-body-->
</div>
<!--end of kt-portlet-->